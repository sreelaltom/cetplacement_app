{
  "version": 2,
  "builds": [
    {
      "src": "hub/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "hub/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hub.production_settings",
    "DEBUG": "False"
  }
}
