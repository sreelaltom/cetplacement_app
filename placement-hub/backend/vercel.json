{
  "version": 2,
  "builds": [
    {
      "src": "hub/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "hub/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hub.settings",
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "hub/wsgi.py": {
      "includeFiles": "**"
    }
  }
}
