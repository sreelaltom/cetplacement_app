{
  "version": 2,
  "builds": [
    {
      "src": "minimal_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "minimal_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hub.settings",
    "DEBUG": "False",
    "USE_SQLITE": "true"
  }
}
