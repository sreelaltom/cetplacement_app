{
  "version": 2,
  "builds": [
    {
      "src": "hub/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "hub/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "hub.production_settings"
  }
}
